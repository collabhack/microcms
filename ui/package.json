{
  "name": "@collabhack/ui-microcms",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/microcms/microcms.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
    "fix": "eslint '**/*.{ts,tsx}' --fix",
    "clean": "rimraf dist node_modules coverage"
  },
  "dependencies": {
    "@stencil/core": "^2.19.3",
    "@collabhack/model-microcms": "../model"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "puppeteer": "^19.3.0",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "^8.26.0",
    "eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20221021",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "miniflare": "^2.11.0",
    "prettierx": "github:utily/prettierx#utily-20221021",
    "typescript": "^4.8.4"
  },
  "license": "MIT"
}
